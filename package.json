{
  "name": "my-study-planner",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "react-app-rewired test",
    "test:coverage": "react-app-rewired test --coverage --watchAll=false",
    "check-format": "npm run prettier -- --list-different",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|tsx|json)\"",
    "format": "npm run prettier -- --write",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "antd": "4.1.4",
    "axios": "0.19.2",
    "firebase": "7.14.2",
    "history": "4.10.1",
    "less": "^3.11.1",
    "moment": "2.24.0",
    "rc-queue-anim": "^1.8.4",
    "react": "16.13.1",
    "react-app-rewired": "2.1.5",
    "react-big-calendar": "0.21.0",
    "react-cookie": "4.0.3",
    "react-dom": "16.13.1",
    "react-error-boundary": "^1.2.5",
    "react-query": "1.2.9",
    "react-query-devtools": "^1.1.5",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "react-use": "^14.2.0",
    "stop-runaway-react-effects": "^2.0.0",
    "styled-components": "5.1.0",
    "throttle-debounce": "2.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "10.0.4",
    "@types/faker": "^4.1.11",
    "@types/history": "4.7.5",
    "@types/jest": "25.2.1",
    "@types/less": "^3.0.1",
    "@types/moment": "2.13.0",
    "@types/node": "13.13.2",
    "@types/react": "16.9.34",
    "@types/react-big-calendar": "0.20.20",
    "@types/react-dom": "16.9.6",
    "@types/react-router": "5.1.5",
    "@types/react-router-dom": "5.1.4",
    "@types/redux-logger": "3.0.7",
    "@types/styled-components": "5.1.0",
    "@types/throttle-debounce": "2.1.0",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "axios-mock-adapter": "^1.18.1",
    "babel-eslint": "10.x",
    "babel-plugin-import": "1.13.0",
    "babel-polyfill": "^6.26.0",
    "customize-cra": "0.9.1",
    "eslint": "6.x",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "eslint-plugin-testing-library": "^3.1.2",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "is-ci-cli": "^2.0.0",
    "jest-styled-components": "7.0.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "16.13.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!<rootDir>/node_modules/**/*",
      "!<rootDir>/src/test/**/*",
      "!<rootDir>/src/dev-tools/**/*"
    ]
  },
  "resolutions": {
    "jest-environment-jsdom": "^26.0.1"
  }
}
