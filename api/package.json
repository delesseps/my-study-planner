{
  "name": "my-study-planner-api",
  "version": "0.0.1",
  "description": "My Study Planner Bulletproof node api",
  "main": "src/app.ts",
  "author": "Jose Felix",
  "scripts": {
    "build": "tsc",
    "dev": "TZ='America/La_Paz' nodemon",
    "inspect": "TZ='America/La_Paz' nodemon --inspect -L src/app.ts",
    "start": "TZ='America/La_Paz' node build/app.js",
    "test": "snyk test && jest",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "snyk-protect": "snyk protect",
    "prettier": "prettier --write src/**/*",
    "start:docker": "run-script-os",
    "start:docker:win32": "docker-compose up -d",
    "start:docker:default": "sudo docker-compose up -d",
    "build:docker": "run-script-os",
    "build:docker:win32": "docker-compose up --build -d",
    "build:docker:default": "sudo docker-compose up --build -d",
    "stop:docker": "run-script-os",
    "stop:docker:win32": "docker-compose down",
    "stop:docker:default": "sudo docker-compose down",
    "precommit": "lint-staged"
  },
  "keywords": [
    "cron",
    "jobs",
    "js",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "dependencies": {
    "agenda": "^2.0.2",
    "agendash": "^1.0.0",
    "bcrypt": "^3.0.7",
    "bluebird": "^3.5.5",
    "body-parser": "^1.18.2",
    "celebrate": "^11.0.0",
    "cloudinary": "^1.14.0",
    "connect-redis": "^4.0.0",
    "cors": "^2.8.4",
    "crypto-random-string": "^3.0.1",
    "date-fns": "^2.0.0",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^3.1.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.16.1",
    "express-status-monitor": "^1.2.7",
    "firebase-admin": "^8.2.0",
    "googleapis": "^41.0.1",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "newrelic": "^5.11.0",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "snyk": "^1.278.1",
    "typedi": "^0.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/agenda": "2.0.7",
    "@types/bcrypt": "3.0.0",
    "@types/bluebird": "3.5.31",
    "@types/connect-redis": "0.0.14",
    "@types/express": "4.17.6",
    "@types/express-rate-limit": "5.0.0",
    "@types/express-session": "1.17.0",
    "@types/jest": "25.2.2",
    "@types/joi": "14.3.4",
    "@types/lodash": "4.14.151",
    "@types/mongoose": "5.5.34",
    "@types/node": "14.0.1",
    "@types/nodemailer": "6.4.0",
    "@types/redis": "2.8.14",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "is-ci-cli": "^2.0.0",
    "jest": "26.0.1",
    "lint-staged": "^10.2.6",
    "nodemon": "2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "run-script-os": "^1.1.1",
    "ts-jest": "24.2.0",
    "ts-node": "8.10.1",
    "tslint": "5.20.1",
    "typescript": "3.9.2"
  },
  "snyk": true
}
