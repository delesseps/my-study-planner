{
  "name": "my-study-planner-api",
  "version": "0.0.1",
  "description": "My Study Planner Bulletproof node api",
  "main": "src/app.ts",
  "author": "Jose Felix",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env TZ='America/La_Paz' nodemon",
    "inspect": "cross-env TZ='America/La_Paz' nodemon --inspect -L src/app.ts",
    "start": "cross-env TZ='America/La_Paz' node build/app.js",
    "test": "snyk test && jest",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "snyk-protect": "snyk protect",
    "prettier": "prettier --write src/**/*",
    "start:docker": "run-script-os",
    "start:docker:win32": "docker-compose up -d",
    "start:docker:default": "sudo docker-compose up -d",
    "build:docker": "run-script-os",
    "build:docker:win32": "docker-compose up --build -d",
    "build:docker:default": "sudo docker-compose up --build -d",
    "stop:docker": "run-script-os",
    "stop:docker:win32": "docker-compose down",
    "stop:docker:default": "sudo docker-compose down",
    "precommit": "lint-staged"
  },
  "keywords": [
    "cron",
    "jobs",
    "js",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "dependencies": {
    "agenda": "^3.1.0",
    "agendash": "^1.0.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "celebrate": "^11.0.2",
    "cloudinary": "^1.22.0",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.2.0",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^4.0.4",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "express-session": "^1.17.1",
    "express-status-monitor": "^1.3.3",
    "firebase-admin": "^8.12.1",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "newrelic": "^5.11.0",
    "nodemailer": "^6.4.8",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0",
    "passport-local": "^1.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "snyk": "^1.278.1",
    "typedi": "^0.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/agenda": "2.0.8",
    "@types/bcrypt": "3.0.0",
    "@types/bluebird": "3.5.32",
    "@types/connect-redis": "0.0.14",
    "@types/express": "4.17.6",
    "@types/express-rate-limit": "5.0.0",
    "@types/express-session": "1.17.0",
    "@types/hapi__joi": "^17.1.2",
    "@types/jest": "26.0.0",
    "@types/joi": "14.3.4",
    "@types/lodash": "4.14.155",
    "@types/mongoose": "5.7.23",
    "@types/node": "14.0.13",
    "@types/nodemailer": "6.4.0",
    "@types/redis": "2.8.22",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "cross-env": "^7.0.2",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "is-ci-cli": "^2.1.0",
    "jest": "26.0.1",
    "lint-staged": "^10.2.9",
    "nodemon": "2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "run-script-os": "^1.1.1",
    "ts-jest": "26.1.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.9.5"
  },
  "snyk": true
}
